; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "Game",
"language": "GDScript",
"path": "res://src/game/Game.gd"
} ]
_global_script_class_icons={
"Game": ""
}

[application]

config/name="Bomb Party"
run/main_scene="res://src/game/Game.tscn"
config/icon="res://icon.png"

[display]

window/size/width=240
window/size/height=368
window/size/test_width=480
window/size/test_height=736
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="tilemap"
2d_physics/layer_2="bomb"
2d_physics/layer_3="box"
2d_physics/layer_4="player"
